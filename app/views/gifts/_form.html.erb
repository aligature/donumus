<%= form_for :post, url: _target, method: _method do |f| %>
   <p>
   <%= f.label :status %><br>
   <%= f.text_field :status %>
   </p>

   <p>
   <%= f.label :description %><br>
   <%= f.text_field :description %>
   </p>

   <p>
   <%= f.label :link %><br>
   <%= f.text_field :link %>
   </p>

   <p>
   <%= f.submit %>
   </p>
<% end %>
